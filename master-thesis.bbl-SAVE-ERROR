      \strng{namehash}{107f665fcf3111d40d044560e1d2bb30}
      \strng{fullhash}{fd99352a6369284f545c3ee31aca889d}
      \strng{bibnamehash}{107f665fcf3111d40d044560e1d2bb30}
      \strng{authorbibnamehash}{107f665fcf3111d40d044560e1d2bb30}
